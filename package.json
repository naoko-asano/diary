{
  "name": "diary",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "22.17.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "db:migrate": "dotenv -e .env.development.local -- prisma migrate dev",
    "test:db:seed": "tsx --env-file .env.test.local prisma/seed.ts",
    "lint": "next lint",
    "format": "prettier --check .",
    "test": "vitest",
    "e2e": "NODE_ENV=test playwright test"
  },
  "dependencies": {
    "@mantine/core": "^8.1.3",
    "@mantine/hooks": "^8.1.3",
    "@prisma/client": "^6.12.0",
    "next": "15.4.1",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.31.0",
    "@playwright/test": "^1.54.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^24.0.13",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.31.0",
    "eslint-config-next": "15.4.1",
    "eslint-config-prettier": "^10.1.5",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "postcss-preset-mantine": "^1.18.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "3.6.2",
    "prisma": "^6.12.0",
    "tsx": "^4.20.4",
    "typescript": "^5.8.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@9.15.1"
}
